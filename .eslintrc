{
    "extends": "eslint-config-airbnb-base",
    "env": {},
    "rules": {
        "no-console": 0, //could be removed after logger implementation
        "new-cap": 0,
        "no-underscore-dangle": 0,
        "linebreak-style": ["error", "windows"],
        "max-len": [
            "error",
            {
                "code": 120,
                "comments": 160,
                "ignoreUrls": true
            }
        ],
        "no-shadow": 0
    }
}