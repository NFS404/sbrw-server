<UserInfo>
    <% if (data.persona.length > 0) { %>
    <defaultPersonaIdx>1</defaultPersonaIdx>
    <% } else { %>
    <defaultPersonaIdx></defaultPersonaIdx>
    <% } %>
    <personas>
        <% data.persona.forEach(function(p, i) { %>
        <ProfileData>
            <Boost><%= p.boost %></Boost>
            <Cash><%= p.cash %></Cash>
            <IconIndex><%= p.iconIndex %></IconIndex>
            <Level><%= p.level %></Level>
            <Name><%= p.name %></Name>
            <PercentToLevel><%= p.percentToLevel %></PercentToLevel>
            <PersonaId><%= i + 1 %></PersonaId>
            <Rating><%= p.rating %></Rating>
            <Rep><%= p.rep %></Rep>
            <RepAtCurrentLevel><%= p.repAtCurrentLevel %></RepAtCurrentLevel>
            <Score><%= p.score %></Score>
        </ProfileData>
        <% }); %>
    </personas>
    <user>
        <fullGameAccess>false</fullGameAccess>
        <isComplete>false</isComplete>
        <remoteUserId>0</remoteUserId>
        <securityToken><%= data.token %></securityToken>
        <subscribeMsg></subscribeMsg>
        <userId>1</userId>
    </user>
</UserInfo>